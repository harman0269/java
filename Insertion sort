package myjava;


public class insertionsort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {9,8,7,14,0};
		for(int i=0;i<=arr.length;i++) {
			int j=-1;
			while(j>=0) {
				if(arr[j]>i) {
					int temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
					
				}
				j--;
			}
			i++;
		}

	}

}
